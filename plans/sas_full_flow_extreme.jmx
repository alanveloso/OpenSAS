          <stringProp name="parameters"></stringProp>
          <stringProp name="script">// Generate valid 42-character Ethereum addresses
import java.util.concurrent.atomic.AtomicInteger;

// Use thread number to generate unique addresses
int threadNum = ctx.getThreadNum();
String baseAddress = "0xf39Fd6e51aad88F6F4ce6aB8827279cffFb9226";

// Create unique addresses by modifying the last few characters
String uniqueSuffix = String.format("%02d", threadNum);
String sasAddress = baseAddress.substring(0, 40) + uniqueSuffix;

// Store in variables
vars.put("sas_address", sasAddress);
vars.put("unique_id", "ID-" + threadNum + "-" + vars.get("iteration_counter"));

log.info("Generated SAS address for thread " + threadNum + ": " + sasAddress);</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
